<%- include('partials/header')%>
    <div class="container">
        <div class="row">
            <div class="col m6 s12 offset-m3">
                <div class="card-panel">
                    <h6 class="center-align">Login</h6>
                    <form action="/login" method="POST">
                        <div class="row">
                            <div class="input-field col s12 offset-m1 m10">
                                <input type="text" class="text-red" name="username">
                                <label for="username">Username</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12 m10 offset-m1">
                                <input type="password" class="text-red" name="password">
                                <label for="password">Password</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col s12 m12 center-align">
                                <button class="btn waves-effect waves-light red" type="submit">Sign In
                                <i class="material-icons right">send</i>
                            </button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
<%- include('partials/footer')%>
